<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity14">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        >
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            >
            <androidx.cardview.widget.CardView
                android:layout_margin="12dp"
                app:cardCornerRadius="20dp"
                app:cardElevation="10dp"
                android:backgroundTint="#F8F9F9"
                android:layout_weight="5"
                android:layout_gravity="center"
                android:layout_width="match_parent"
                android:layout_height="170dp"
                >

                <LinearLayout
                    android:id="@+id/jahangir"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    >
                    <TextView
                        android:layout_margin="5dp"
                        android:id="@+id/textView3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="20sp"
                        android:textColor="@color/black"
                        android:textStyle="bold"
                        android:layout_gravity="center"
                        android:text="ডাঃজাহাঙ্গীর আলম" />

                    <TextView
                        android:layout_margin="5dp"
                        android:id="@+id/textView3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="15sp"
                        android:textColor="#229954"
                        android:textStyle="bold"
                        android:layout_gravity="center"
                        android:text="বিডিএস বিসিএস( স্বাস্থ্য)  " />

                    <TextView
                        android:id="@+id/textView3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="15sp"
                        android:textColor="@color/black"
                        android:textStyle="bold"
                        android:layout_gravity="center"
                        android:text="দন্ত রোগ বিশেষজ্ঞ ও সার্জন "/>

                    <TextView
                        android:layout_margin="5dp"
                        android:id="@+id/textView3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="15sp"
                        android:textColor="#FF7F50"
                        android:textStyle="bold"
                        android:layout_gravity="center"
                        android:text="চেম্বারঃ ইউ এস ডেন্টাল কেয়ার" />

                    <ImageView

                        android:layout_width="match_parent"
                        android:layout_height="30dp"
                        app:srcCompat="@drawable/call" />


                </LinearLayout>



            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:layout_margin="12dp"
                app:cardCornerRadius="20dp"
                app:cardElevation="10dp"
                android:backgroundTint="#F8F9F9"
                android:layout_weight="5"
                android:layout_gravity="center"
                android:layout_width="match_parent"
                android:layout_height="180dp"
                >

                <LinearLayout
                    android:id="@+id/tanuj"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    >
                    <TextView
                        android:layout_margin="5dp"
                        android:id="@+id/textView3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="20sp"
                        android:textColor="@color/black"
                        android:textStyle="bold"
                        android:layout_gravity="center"
                        android:text="ডাঃ তনুজ ভৌমিক" />

                    <TextView
                        android:layout_margin="5dp"
                        android:id="@+id/textView3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="14sp"
                        android:textColor="#229954"
                        android:textStyle="bold"
                        android:layout_gravity="center"
                        android:text="বিডিএস (ঢাকা) পিজিটি(ডেন্টিস্ট্রি) পিজিটি(ও এম এস) " />

                    <TextView
                        android:id="@+id/textView3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="15sp"
                        android:textColor="@color/black"
                        android:textStyle="bold"
                        android:layout_gravity="center"
                        android:text=" দন্ত রোগ বিশেষজ্ঞ "/>

                    <TextView
                        android:layout_margin="5dp"
                        android:id="@+id/textView3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="15sp"
                        android:textColor="#FF7F50"
                        android:textStyle="bold"
                        android:layout_gravity="center"
                        android:text="চেম্বারঃ Family Dental Care(গোয়ালপট্টি)" />

                    <ImageView

                        android:layout_width="match_parent"
                        android:layout_height="30dp"
                        app:srcCompat="@drawable/call" />


                </LinearLayout>



            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:layout_margin="12dp"
                app:cardCornerRadius="20dp"
                app:cardElevation="10dp"
                android:backgroundTint="#F8F9F9"
                android:layout_weight="5"
                android:layout_gravity="center"
                android:layout_width="match_parent"
                android:layout_height="180dp"
                >

                <LinearLayout
                    android:id="@+id/rayhan"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    >
                    <TextView
                        android:layout_margin="5dp"
                        android:id="@+id/textView3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="20sp"
                        android:textColor="@color/black"
                        android:textStyle="bold"
                        android:layout_gravity="center"
                        android:text="ডাঃমোঃ আশরাকুর রহমান  রায়হান" />

                    <TextView
                        android:layout_margin="5dp"
                        android:id="@+id/textView3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="13sp"
                        android:textColor="#229954"
                        android:textStyle="bold"
                        android:layout_gravity="center"
                        android:text="বিডিএস(ঢাকা) বিসিএস (স্বাস্থ্য) এম এস(অরথোডন্টিক্স) " />

                    <TextView
                        android:id="@+id/textView3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="14sp"
                        android:textColor="@color/black"
                        android:textStyle="bold"
                        android:layout_gravity="center"
                        android:text="বিশেষজ্ঞ ডেন্টাল সার্জন ও লেকচারার "/>
                    <TextView
                        android:id="@+id/textView3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="13sp"
                        android:textColor="@color/black"
                        android:textStyle="bold"
                        android:layout_gravity="center"
                        android:text="ঢাকা মেডিকেল কলেজ হাসপাতাল  "/>

                    <TextView
                        android:layout_margin="5dp"
                        android:id="@+id/textView3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="13sp"
                        android:textColor="#FF7F50"
                        android:textStyle="bold"
                        android:layout_gravity="center"
                        android:text="চেম্বারঃ আইডিয়াল ডেন্টাল হোম , নিউমার্কেট" />

                    <ImageView

                        android:layout_width="match_parent"
                        android:layout_height="30dp"
                        app:srcCompat="@drawable/call" />


                </LinearLayout>



            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:layout_margin="12dp"
                app:cardCornerRadius="20dp"
                app:cardElevation="10dp"
                android:backgroundTint="#F8F9F9"
                android:layout_weight="5"
                android:layout_gravity="center"
                android:layout_width="match_parent"
                android:layout_height="170dp"
                >

                <LinearLayout
                    android:id="@+id/avijit"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    >
                    <TextView
                        android:layout_margin="5dp"
                        android:id="@+id/textView3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="20sp"
                        android:textColor="@color/black"
                        android:textStyle="bold"
                        android:layout_gravity="center"
                        android:text="ডাঃঅভিজিৎ কুমার ঘোষ" />

                    <TextView
                        android:layout_margin="5dp"
                        android:id="@+id/textView3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="15sp"
                        android:textColor="#229954"
                        android:textStyle="bold"
                        android:layout_gravity="center"
                        android:text="বিডিএস (ঢাকা)  এফসিপিএস(শেষ পর্ব)  " />

                    <TextView
                        android:id="@+id/textView3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="15sp"
                        android:textColor="@color/black"
                        android:textStyle="bold"
                        android:layout_gravity="center"
                        android:text="মেডিডেন্ট ডেন্টাল ও ম্যাক্সিলো সার্জারি  "/>

                    <TextView
                        android:layout_margin="5dp"
                        android:id="@+id/textView3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="15sp"
                        android:textColor="#FF7F50"
                        android:textStyle="bold"
                        android:layout_gravity="center"
                        android:text="চেম্বারঃচকবাজার,থানামোড়" />

                    <ImageView

                        android:layout_width="match_parent"
                        android:layout_height="30dp"
                        app:srcCompat="@drawable/call" />


                </LinearLayout>



            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:layout_margin="12dp"
                app:cardCornerRadius="20dp"
                app:cardElevation="10dp"
                android:backgroundTint="#F8F9F9"
                android:layout_weight="5"
                android:layout_gravity="center"
                android:layout_width="match_parent"
                android:layout_height="180dp"
                >

                <LinearLayout
                    android:id="@+id/abdullah"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    >
                    <TextView
                        android:layout_margin="5dp"
                        android:id="@+id/textView3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="20sp"
                        android:textColor="@color/black"
                        android:textStyle="bold"
                        android:layout_gravity="center"
                        android:text="ডাঃমোঃ আব্দুল্লাহ আল মামুন" />

                    <TextView
                        android:layout_margin="5dp"
                        android:id="@+id/textView3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="15sp"
                        android:textColor="#229954"
                        android:textStyle="bold"
                        android:layout_gravity="center"
                        android:text="এমবিবিএস ডিও(ডি ইউ) " />

                    <TextView
                        android:id="@+id/textView3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="12sp"
                        android:textColor="@color/black"
                        android:textStyle="bold"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:text="বিডিএস  পিজিটি (ওরাল এন্ড ম্যাক্সিলোফেসিয়াল সার্জারি ) পিজিটি(কঞ্জারভেটিভ ডেন্টিস্ট্রি) "/>

                    <TextView
                        android:layout_margin="5dp"
                        android:id="@+id/textView3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="15sp"
                        android:textColor="#FF7F50"
                        android:textStyle="bold"
                        android:layout_gravity="center"
                        android:text="চেম্বারঃ গোপালবাড়ি,বটতলা,শেরপুর সদর" />

                    <ImageView

                        android:layout_width="match_parent"
                        android:layout_height="30dp"
                        app:srcCompat="@drawable/call" />


                </LinearLayout>



            </androidx.cardview.widget.CardView>






        </LinearLayout>
    </ScrollView>
</LinearLayout>